pipeline {
  agent any
  stages {
    stage('test') {
      steps {
        sh 'echo "my name is Pranaya"'
        sh '"ls"'
      }
    }
    stage('dev') {
      steps {
        sh 'echo "Test"'
        sh '"ls -a"'
      }
    }
    stage('123') {
      steps {
        sh 'echo "test test"'
      }
    }
  }
}